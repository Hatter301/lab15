#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,j,min,max,k;
	int* a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		min = INT_MAX;
		max = -1 * INT_MAX;
		a = (int*)malloc(n * sizeof(int));
		for (i=0; i < n; i++) 
			cin >> a[i];
		for (i = 0; i < n; i++) {
			if (a[i] > max) {
				max = a[i];
				k = i;
			}
			if (a[i] < min) {
				min = a[i];
				j = i;
			}
		}
		if (j > k) {
			k = k + 1;
			for (k; k < j; k++)
				a[k] = 0;
		}
		else if (k > j) {
			j = j + 1;
			for (j; j < k; j++)
				a[j] = 0;
		}
		for (i = 0; i < n; i++)
			cout << a[i] << " ";
		free(a);
	}
	return 0;
}
