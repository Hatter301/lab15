#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,t;
	int* a;
	int* b;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		a = (int*)malloc(n * sizeof(int));
		b = (int*)malloc(n * sizeof(int));
		for (i=0; i < n; i++) 
			cin >> a[i];
		for (i=0; i < n; i++)
			cin >> b[i];
		for (i = 0; i < n; i++) {
			t = a[i];
			a[i] = b[i];
			b[i] = t;
		}
		for (i = 0; i < n; i++)
			cout << a[i] << " ";
		cout<<endl;
		for (i = 0; i < n; i++)
			cout << b[i] << " ";
		free(a);
	}
	return 0;
}
