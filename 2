#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,k;
	double s = 0;
	int* a;
	double* b;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		a = (int*)malloc(n * sizeof(int));
		b = (double*)malloc(n * sizeof(double));
		for (i=0; i < n; i++) 
			cin >> a[i];
		for (i = 0; i < n; i++) {
			for (k = 0; k <= i; k++)
				s = s + a[k];
			b[i] = s/k;
			s = 0;
			k = 0;
		}
		for (i = 0; i < n; i++)
			cout << b[i] << " ";
		free(a);
	}
	return 0;
}
