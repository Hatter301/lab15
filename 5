#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,j,f=0,t;
	int* a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		a = (int*)malloc(n * sizeof(int));
		for (i=0; i < n; i++) 
			cin >> a[i];
		for (i = 1; i < n - 1; i++)
			if (a[i] > a[i + 1])
				f = 1;
		if ((f == 1) or (a[0]<=a[1]))
			cout << "Данные введены некорректно";
		else {
			for (i = 1; i < n-1; i++)
				if ((a[0] >= a[i]) and (a[0] <= a[i + 1])) {
					t = a[0];
					for (j = 0; j < i; j++) 
						a[j] = a[j + 1];
					a[i] = t;
					i = n - 2;
				}
			for (i = 0; i < n; i++)
				cout << a[i] << " ";
		}
		free(a);
	}
	return 0;
}
