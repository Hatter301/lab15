#include <iostream>
#include <cstdlib>
using namespace std;
int main() {
	int i, n,f=-1;
	int* a;
	cin >> n;
	if (n <= 0)
		cout << "Данные введены некорректно";
	else {
		a = (int*)malloc(n * sizeof(int));
		for (i=0; i < n; i++) 
			cin >> a[i];
		for (i = 0; i < n; i++)
			if ((a[i] % 2) != 0)
				f = a[i];
		if (f == -1)
			for (i = 0; i < n; i++)
				cout << a[i] << " ";
		else {
			for (i = 0; i < n; i++)
				if ((a[i] % 2) != 0)
					a[i] = a[i] + f;
			for (i = 0; i < n; i++)
				cout << a[i] << " ";
		}
		free(a);
	}
	return 0;
}
